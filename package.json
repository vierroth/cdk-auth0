{
	"name": "@flit/cdk-auth0",
	"version": "1.16.1",
	"description": "A collection of AWS CDK constructs to manager your Auth0 resources programmatically",
	"keywords": [
		"aws",
		"cdk",
		"awscdk",
		"aws-cdk",
		"auth0",
		"oauth",
		"oauth2",
		"authentication",
		"jwt",
		"okta"
	],
	"homepage": "https://github.com/vierroth/cdk-auth0",
	"bugs": "https://github.com/vierroth/cdk-auth0/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/vierroth/cdk-auth0"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Vierroth GmbH",
		"url": "https://github.com/vierroth"
	},
	"contributors": [
		{
			"name": "Luis Vierroth",
			"url": "https://github.com/p-mercury"
		},
		{
			"name": "Cameron Moyer",
			"url": "https://github.com/cmoyer2"
		}
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"./src",
		"./.jsii",
		"./LICENSE.txt",
		"./README.md"
	],
	"scripts": {
		"build": "jsii",
		"prepack": "jsii"
	},
	"prettier": {
		"plugins": [
			"prettier-plugin-packagejson"
		],
		"useTabs": true
	},
	"dependencies": {
		"auth0": "^4.29.0"
	},
	"devDependencies": {
		"@aws-sdk/client-secrets-manager": "^3.886.0",
		"@types/aws-lambda": "^8.10.152",
		"@types/node": "^24.3.1",
		"aws-cdk-lib": "^2.140.0",
		"constructs": "^10.4.0",
		"jest": "^30.1.3",
		"jsii": "^5.9.4",
		"jsii-pacmak": "^1.114.1",
		"prettier": "^3.6.2",
		"prettier-plugin-packagejson": "^2.5.19",
		"ts-jest": "^29.4.1",
		"typescript": "^5.9.2"
	},
	"peerDependencies": {
		"aws-cdk-lib": "^2.140.0",
		"constructs": "^10.4.0"
	},
	"bundleDependencies": [
		"auth0"
	],
	"publishConfig": {
		"access": "public"
	},
	"jsii": {
		"outdir": "./dist",
		"excludeTypescript": [
			"**/handler.ts"
		],
		"tsc": {
			"rootDir": "./src",
			"outDir": "./dist"
		},
		"targets": {
			"java": {
				"package": "flit.cdk-auth0",
				"maven": {
					"groupId": "flit",
					"artifactId": "cdk-auth0"
				}
			},
			"dotnet": {
				"namespace": "Flit.CDK.Auth0",
				"packageId": "Flit.CDK.Auth0"
			},
			"python": {
				"libName": "flit.cdk-auth0",
				"module": "flit.cdk-auth0",
				"classifiers": [
					"Framework :: AWS CDK",
					"Framework :: AWS CDK :: 2"
				]
			}
		}
	},
	"stability": "stable"
}
